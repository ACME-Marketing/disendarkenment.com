<script>
	// FAQ Toggle functionality
	document.addEventListener('DOMContentLoaded', function() {
		const faqToggles = document.querySelectorAll('.faq-toggle');
		
		faqToggles.forEach(toggle => {
			toggle.addEventListener('click', function() {
				const targetId = this.getAttribute('data-target');
				const targetElement = document.getElementById(targetId);
				const arrow = this.querySelector('svg');
				
				if (targetElement) {
					if (targetElement.classList.contains('hidden')) {
						targetElement.classList.remove('hidden');
						arrow.style.transform = 'rotate(180deg)';
					} else {
						targetElement.classList.add('hidden');
						arrow.style.transform = 'rotate(0deg)';
					}
				}
			});
		});

		// Package selection functionality
		const packageButtons = document.querySelectorAll('button[class*="Choose"]');
		packageButtons.forEach(button => {
			button.addEventListener('click', function() {
				const packageName = this.textContent.replace('Choose ', '');
				// Here you would typically redirect to a booking/contact form
				// For now, we'll show an alert
				alert(`You've selected the ${packageName} package. Redirecting to consultation booking...`);
				// In a real implementation, you might do:
				// window.location.href = `/get-started?package=${packageName.toLowerCase()}`;
			});
		});
	});
</script>